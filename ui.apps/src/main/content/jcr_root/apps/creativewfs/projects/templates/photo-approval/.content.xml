<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="Creates a Project for Photo Approval Process"
    jcr:primaryType="cq:Template"
    jcr:title="Photo Approval"
    includeInCreateProject="{Boolean}true"
    ranking="{Long}1"
    wizard="/libs/cq/core/content/projects/wizard/steps/defaultproject.html">
    <jcr:content
        jcr:primaryType="nt:unstructured"
        detailsHref="/projects/details.html"/>
    <gadgets jcr:primaryType="nt:unstructured">
        <projectinfo
            jcr:primaryType="nt:unstructured"
            jcr:title="Project Info"
            sling:resourceType="cq/gui/components/projects/admin/pod/projectinfopod"
            cardWeight="50"/>
        <team
            jcr:primaryType="nt:unstructured"
            jcr:title="Team"
            sling:resourceType="cq/gui/components/projects/admin/pod/teampod"
            cardWeight="60"/>
        <work
            jcr:primaryType="nt:unstructured"
            jcr:title="Workflows"
            sling:resourceType="cq/gui/components/projects/admin/pod/workpod"
            cardWeight="80"/>
        <tasks
            jcr:primaryType="nt:unstructured"
            jcr:title="Tasks"
            sling:resourceType="cq/gui/components/projects/admin/pod/taskpod"
            cardWeight="100"/>
    </gadgets>
    <roles jcr:primaryType="nt:unstructured">
        <approver
            jcr:primaryType="nt:unstructured"
            jcr:title="Approvers"
            roleclass="editor"
            roleid="approver"/>
        <retoucher
            jcr:primaryType="nt:unstructured"
            jcr:title="Clip and Croppers"
            roleclass="editor"
            roleid="retoucher"/>
    </roles>
    <workflows
        jcr:primaryType="nt:unstructured"
        tags="[]">
        <models jcr:primaryType="nt:unstructured">
            <photoapproval
                jcr:primaryType="nt:unstructured"
                modelId="/var/workflow/models/photo-shoot-review-approval"
                wizard="/apps/AEMaaCS Transition/projects/workflowwizards/photo-approval-wizard.html"/>
        </models>
    </workflows>
</jcr:root>
